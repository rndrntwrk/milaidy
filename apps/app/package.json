{
  "name": "@milady/app",
  "version": "2.0.0-alpha.27",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:electron:e2e": "playwright test --config playwright.electron.config.ts",
    "test:electron:packaged:e2e": "playwright test --config playwright.electron.packaged.config.ts",
    "plugin:build": "for d in gateway swabble camera screencapture canvas desktop location talkmode agent; do echo \"[plugin:$d] building...\" && (cd plugins/$d && ../../../../scripts/rt.sh run build) || exit 1; done",
    "sync:electron:web-assets": "node scripts/sync-electron-web-assets.mjs",
    "build": "../../scripts/rt.sh install && ../../scripts/rt.sh run plugin:build && ../../scripts/rt.sh install && vite build",
    "build:ios": "../../scripts/rt.sh run build && capacitor sync ios",
    "build:android": "../../scripts/rt.sh run build && capacitor sync android",
    "build:electron": "../../scripts/rt.sh run build && ../../scripts/rt.sh run sync:electron:web-assets",
    "ios": "../../scripts/rt.sh run build:ios && capacitor open ios",
    "android": "../../scripts/rt.sh run build:android && capacitor open android",
    "electron": "../../scripts/rt.sh run build:electron && capacitor open @capacitor-community/electron",
    "cap:sync": "capacitor sync",
    "cap:sync:ios": "capacitor sync ios",
    "cap:sync:android": "capacitor sync android",
    "cap:sync:electron": "node scripts/sync-electron-web-assets.mjs",
    "cap:open:ios": "capacitor open ios",
    "cap:open:android": "capacitor open android",
    "cap:open:electron": "capacitor open @capacitor-community/electron"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.1",
    "@capacitor/core": "8.0.2",
    "@capacitor/haptics": "8.0.0",
    "@capacitor/ios": "^8.0.2",
    "@capacitor/keyboard": "8.0.0",
    "@capacitor/preferences": "^8.0.1",
    "@capacitor/status-bar": "^8.0.1",
    "@milady/capacitor-agent": "workspace:*",
    "@milady/capacitor-camera": "workspace:*",
    "@milady/capacitor-canvas": "workspace:*",
    "@milady/capacitor-desktop": "workspace:*",
    "@milady/capacitor-gateway": "workspace:*",
    "@milady/capacitor-location": "workspace:*",
    "@milady/capacitor-screencapture": "workspace:*",
    "@milady/capacitor-swabble": "workspace:*",
    "@milady/capacitor-talkmode": "workspace:*",
    "@noble/curves": "^2.0.1",
    "@pixiv/three-vrm": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "three": "^0.182.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.2",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.1.3",
    "jsdom": "^28.1.0",
    "react-test-renderer": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  }
}
