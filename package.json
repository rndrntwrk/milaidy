{
  "name": "milady",
  "version": "2.0.0-alpha.22",
  "description": "Cute agents for the acceleration",
  "homepage": "https://github.com/milady-ai/milady",
  "author": {
    "name": "Milady AI",
    "email": "hi@milady.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milady-ai/milady.git"
  },
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "apps/*/plugins/*",
    "apps/*/electron",
    "apps/app/electron",
    "deploy/cloud-agent-template",
    "plugins/plugin-*"
  ],
  "type": "module",
  "main": "build/src/index.js",
  "bin": {
    "milady": "milady.mjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli-entry": "./milady.mjs",
    "./eliza": "./dist/runtime/eliza.js"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "benchmark:server": "node --import tsx src/benchmark/server.ts",
    "build": "tsdown && scripts/rt.sh scripts/write-build-info.ts && cd apps/app && ../../scripts/rt.sh run build",
    "build:node": "tsdown && node --import tsx scripts/write-build-info.ts && cd apps/app && npm run build",
    "build:android": "cd apps/app && ../../scripts/rt.sh run build:android",
    "build:desktop": "cd apps/app && ../../scripts/rt.sh run build:electron",
    "build:ios": "cd apps/app && ../../scripts/rt.sh run build:ios",
    "check": "scripts/rt.sh run typecheck && scripts/rt.sh run lint",
    "dev": "scripts/rt.sh scripts/dev-ui.mjs",
    "dev:all": "scripts/rt.sh scripts/dev-all.mjs",
    "dev:android": "cd apps/app && ../../scripts/rt.sh run android",
    "dev:desktop": "cd apps/app/electron && ../../../scripts/rt.sh run electron:start-live",
    "dev:ios": "cd apps/app && ../../scripts/rt.sh run ios",
    "dev:ui": "scripts/rt.sh scripts/dev-ui.mjs --ui-only",
    "docs:bin": "scripts/rt.sh scripts/docs-list.js --build-bin",
    "docs:build": "cd docs && bun exec mint broken-links",
    "docs:dev": "cd docs && mint dev",
    "docs:list": "scripts/rt.sh scripts/docs-list.js",
    "format": "bunx @biomejs/biome format src scripts apps",
    "format:fix": "bunx @biomejs/biome format --write src scripts apps",
    "lint": "bunx @biomejs/biome check src scripts apps",
    "lint:fix": "bunx @biomejs/biome check --write src scripts apps",
    "generate:plugins": "node scripts/generate-plugin-index.js",
    "learn:loop": "scripts/rt.sh scripts/knowledge-loop.mjs",
    "learn:snapshot": "scripts/rt.sh scripts/knowledge-loop.mjs --full",
    "milady": "scripts/rt.sh scripts/run-node.mjs",
    "postinstall": "node ./scripts/init-submodules.mjs && node ./scripts/ensure-skills.mjs && node ./scripts/link-browser-server.mjs && node ./scripts/patch-deps.mjs",
    "prepack": "scripts/rt.sh run build",
    "prepare": "node -e \"try{require('child_process').execSync('git config core.hooksPath git-hooks',{stdio:'ignore'})}catch{}\"",
    "publish:all:next": "node ./scripts/publish-all-next.mjs",
    "publish:plugins:next": "cd plugins && bun run publish:next",
    "release:check": "node --import tsx scripts/release-check.ts",
    "start": "scripts/rt.sh scripts/run-node.mjs start",
    "start:desktop": "cd apps/app && ../../scripts/rt.sh run build:electron && cd electron && ../../../scripts/rt.sh run electron:start",
    "start:eliza": "node --import tsx src/runtime/eliza.ts",
    "test": "node test/scripts/test-parallel.mjs",
    "test:coverage": "bunx vitest run --coverage",
    "test:docker:all": "bun run test:docker:live-models && bun run test:docker:live-gateway && bun run test:docker:onboard && bun run test:docker:gateway-network && bun run test:docker:qr && bun run test:docker:doctor-switch && bun run test:docker:plugins && bun run test:docker:cleanup",
    "test:docker:cleanup": "bash test/scripts/test-cleanup-docker.sh",
    "test:docker:doctor-switch": "bash test/scripts/e2e/doctor-install-switch-docker.sh",
    "test:docker:gateway-network": "bash test/scripts/e2e/gateway-network-docker.sh",
    "test:docker:live-gateway": "bash test/scripts/test-live-gateway-models-docker.sh",
    "test:docker:live-models": "bash test/scripts/test-live-models-docker.sh",
    "test:docker:onboard": "bash test/scripts/e2e/onboard-docker.sh",
    "test:docker:plugins": "bash test/scripts/e2e/plugins-docker.sh",
    "test:docker:qr": "bash test/scripts/e2e/qr-import-docker.sh",
    "test:docker:fresh-install": "bash test/scripts/e2e/run-e2e-fresh-install.sh",
    "test:docker:cli-boot": "bash test/scripts/e2e/run-e2e-cli-boot.sh",
    "test:docker:stress": "bash test/scripts/e2e/run-e2e-stress-test.sh",
    "test:e2e": "bunx vitest run --config vitest.e2e.config.ts",
    "test:force": "node --import tsx test/scripts/test-force.ts",
    "test:install:e2e": "bash test/scripts/test-install-sh-e2e-docker.sh",
    "test:install:e2e:anthropic": "MILADY_E2E_MODELS=anthropic bash test/scripts/test-install-sh-e2e-docker.sh",
    "test:install:e2e:openai": "MILADY_E2E_MODELS=openai bash test/scripts/test-install-sh-e2e-docker.sh",
    "test:install:smoke": "bash test/scripts/test-install-sh-docker.sh",
    "test:live": "MILADY_LIVE_TEST=1 bun exec vitest run --config vitest.e2e.config.ts test/wallet-live.e2e.test.ts test/api-auth-live.e2e.test.ts test/cloud-providers.e2e.test.ts",
    "test:watch": "bunx vitest",
    "typecheck": "tsc --noEmit",
    "tui": "scripts/rt.sh scripts/run-node.mjs tui",
    "tui:dev": "MILADY_PROFILE=dev scripts/rt.sh scripts/run-node.mjs --dev tui"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.11.0",
    "@capacitor-community/electron": "^5.0.1",
    "@clack/prompts": "^1.0.0",
    "@elizaos/core": "next",
    "@elizaos/plugin-agent-orchestrator": "next",
    "@elizaos/plugin-agent-skills": "next",
    "@elizaos/plugin-browser": "next",
    "@elizaos/plugin-computeruse": "next",
    "@elizaos/plugin-cron": "next",
    "@elizaos/plugin-discord": "next",
    "@elizaos/plugin-edge-tts": "next",
    "@elizaos/plugin-elevenlabs": "next",
    "@elizaos/plugin-elizacloud": "next",
    "@elizaos/plugin-experience": "next",
    "@elizaos/plugin-form": "next",
    "@elizaos/plugin-google-genai": "next",
    "@elizaos/plugin-groq": "next",
    "@elizaos/plugin-knowledge": "next",
    "@elizaos/plugin-local-embedding": "next",
    "@elizaos/plugin-ollama": "next",
    "@elizaos/plugin-openai": "next",
    "@elizaos/plugin-openrouter": "next",
    "@elizaos/plugin-pdf": "next",
    "@elizaos/plugin-personality": "next",
    "@elizaos/plugin-plugin-manager": "next",
    "@elizaos/plugin-rolodex": "next",
    "@elizaos/plugin-secrets-manager": "next",
    "@elizaos/plugin-shell": "next",
    "@elizaos/plugin-telegram": "next",
    "@elizaos/plugin-todo": "next",
    "@elizaos/plugin-trajectory-logger": "next",
    "@elizaos/plugin-trust": "next",
    "@elizaos/prompts": "next",
    "@elizaos/sweagent-root": "next",
    "@elizaos/tui": "next",
    "@langchain/core": "^1.1.12",
    "@langchain/textsplitters": "^1.0.1",
    "@noble/curves": "^2.0.1",
    "adze": "^2.2.5",
    "bonjour-service": "^1.3.0",
    "chalk": "^5.4.1",
    "chokidar": "~3.5.3",
    "commander": "^14.0.0",
    "cross-spawn": "^7.0.6",
    "crypto-browserify": "^3.12.0",
    "dedent": "^1.7.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "0.45.1",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~4.0.1",
    "electron-updater": "^6.8.3",
    "electron-window-state": "^5.0.3",
    "ethers": "^6.16.0",
    "fast-redact": "^3.5.0",
    "file-type": "^21.3.0",
    "glob": "^13.0.0",
    "handlebars": "^4.7.8",
    "json5": "^2.2.3",
    "markdown-it": "^14.1.0",
    "node-llama-cpp": "^3.15.1",
    "pdfjs-dist": "^5.4.530",
    "pg": "^8.16.3",
    "telegraf": "^4.16.3",
    "unique-names-generator": "^4.7.1",
    "whisper-node": "^1.1.1",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.2.0",
    "@types/pg": "^8.15.2",
    "@types/ws": "^8.18.1",
    "@types/markdown-it": "^14.1.2",
    "@types/cross-spawn": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.18",
    "react-test-renderer": "^19.2.4",
    "rimraf": "^5.0.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "drizzle-orm": "0.45.1",
    "ai": "6.0.30",
    "tar": "^7.5.7",
    "@elizaos/core": "next",
    "@elizaos/prompts": "next"
  }
}