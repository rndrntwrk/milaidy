{
  "name": "Milaidy",
  "version": "2.0.0-alpha.7",
  "description": "Cute agents for the acceleration",
  "homepage": "https://github.com/milady-ai/milaidy",
  "author": {
    "name": "Milady AI",
    "email": "hi@milady.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milady-ai/milaidy.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "build/src/index.js",
  "bin": {
    "milaidy": "milaidy.mjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli-entry": "./milaidy.mjs",
    "./eliza": "./dist/runtime/eliza.js"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "bunx tsdown && cp src/plugins/telegram-enhanced/package.json dist/plugins/telegram-enhanced/package.json && scripts/rt.sh scripts/write-build-info.ts && cd apps/app && ../../scripts/rt.sh run build",
    "check": "scripts/rt.sh run typecheck && scripts/rt.sh run lint",
    "format": "bunx @biomejs/biome format .",
    "lint": "bunx @biomejs/biome check .",
    "milaidy": "scripts/rt.sh scripts/run-node.mjs",
    "postinstall": "node ./scripts/link-browser-server.mjs",
    "start": "scripts/rt.sh scripts/run-node.mjs start",
    "test": "node test/scripts/test-parallel.mjs",
    "test:coverage": "bunx vitest run --coverage",
    "test:e2e": "bunx vitest run --config vitest.e2e.config.ts",
    "test:live": "MILAIDY_LIVE_TEST=1 bunx vitest run --config vitest.e2e.config.ts test/wallet-live.e2e.test.ts test/api-auth-live.e2e.test.ts test/cloud-providers.e2e.test.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@elizaos/core": "2.0.0-alpha.4",
    "@elizaos/plugin-google-genai": "2.0.0-alpha.4",
    "@elizaos/plugin-knowledge": "2.0.0-alpha.4",
    "@elizaos/plugin-ollama": "2.0.0-alpha.4",
    "@elizaos/plugin-openai": "2.0.0-alpha.4",
    "@elizaos/plugin-openrouter": "2.0.0-alpha.4",
    "@capacitor-community/electron": "^5.0.1",
    "bonjour-service": "^1.3.0",
    "chalk": "^5.4.1",
    "chokidar": "~3.5.3",
    "commander": "^14.0.0",
    "dotenv": "^17.2.3",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~4.0.1",
    "electron-updater": "^5.3.0",
    "electron-window-state": "^5.0.3",
    "json5": "^2.2.3",
    "whisper-node": "^1.1.1",
    "zod": "^4.3.6"
  }
}
