#!/bin/sh
set -e

case "$1" in
    configure)
        echo ""
        echo "  Milaidy installed! Get started:"
        echo "    milaidy start       — Start the agent runtime"
        echo "    milaidy setup       — Initialize workspace"
        echo "    milaidy --help      — Show all commands"
        echo ""
        ;;
esac

#DEBHELPER#

exit 0
